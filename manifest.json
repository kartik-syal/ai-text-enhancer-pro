{
  "manifest_version": 3,
  "name": "AutoFill Extension",
  "version": "1.0",
  "description": "Autofill text inputs using ChatGPT",
  "permissions": ["activeTab", "storage"],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "icon.png",
          "48": "icon.png",
          "128": "icon.png"
      }
  },
  "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["content.js"],
          "css": ["styles.css"]
      }
  ]
}
